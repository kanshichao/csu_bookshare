  "keywords": [
    "wechat",
    "csu_bookshare",
    "book",
    "management"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "dependencies": {
    "weui-csu_bookshare": "^1.2.3",
    "dayjs": "^1.11.5",
    "csu_bookshare-api-typings": "^3.6.0",
    "csu_bookshare-computed": "^4.1.1",
    "csu_bookshare-recycle-view": "^0.1.5",
    "wechat-csu_bookshare-request": "^1.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-wechat-csu_bookshare": "^1.1.1",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "jest": "^27.5.1",
    "csu_bookshare-ci": "^1.8.0",
    "csu_bookshare-simulate": "^1.5.0",
    "postcss": "^8.4.12",
    "postcss-pxtransform": "^3.8.0",
