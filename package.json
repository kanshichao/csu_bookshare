  "description": "寝室图书共享小程序前端",
  "main": "app.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development gulp",
    "build": "cross-env NODE_ENV=production gulp build",
    "lint": "eslint --ext .js,.wxml,.wxss ./csu_bookshare",
    "fix": "eslint --fix --ext .js,.wxml,.wxss ./csu_bookshare",
    "test": "jest",
    "upload": "csu_bookshare-ci upload",
